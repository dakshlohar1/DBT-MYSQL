{
    "extends": [
        "eslint:recommended",
        "airbnb-base",
        "airbnb-typescript/base",
        "prettier",
        "plugin:json/recommended"
    ],
    "plugins": ["@typescript-eslint"],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "no-console": "off",
        "import/prefer-default-export": "off",
        "no-restricted-syntax": "off",
        "no-throw-literal": "off"
    }
}
