{
    "extends": [
        "eslint:recommended",
        "airbnb-base",
        "airbnb-typescript/base",
        "prettier",
        "plugin:json/recommended"
    ],
    "plugins": ["@typescript-eslint"],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "no-case-declarations": "off",
        "no-template-curly-in-string": "off"
    }
}
